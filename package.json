{
  "name": "storybook-addon-jira",
  "version": "1.0.0",
  "description": "This addon allows the user to link JIRA Ticket information to stories via the parameters object.",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JRSoftware92/storybook-addon-jira.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JRSoftware92/storybook-addon-jira/issues"
  },
  "homepage": "https://github.com/JRSoftware92/storybook-addon-jira#readme",
  "scripts": {
    "test": "react-scripts test",
    "test:with-reports": "react-scripts test --json --outputFile=jest-report.json --watchAll=false --collectCoverage",
    "eject": "react-scripts eject",
    "storybook": "start-storybook"
  },
  "dependencies": {
    "jira2md": "^2.1.0",
    "lodash": "^4.17.21",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-markdown": "^7.0.0",
    "react-scripts": "3.4.1"
  },
  "devDependencies": {
    "@storybook/addons": "^6.3.7",
    "@storybook/components": "^6.3.7",
    "@storybook/core-events": "^6.3.7",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^5.3.18",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.{spec,test}.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/*.{story,stories}.js",
      "!src/test/**/*",
      "!src/stories/**/*",
      "!src/setupTests.js"
    ],
    "coverageReporters": [
      "json",
      "json-summary",
      "text",
      "text-summary",
      "lcov"
    ]
  }
}
